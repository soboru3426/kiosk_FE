<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.kiosk.branch.BranchMapper">

    <select id="findByBranchName" parameterType="String" resultType="com.example.kiosk.branch.Branch">
        SELECT * 
        FROM branch 
        WHERE branch_name = #{branchName}
    </select>

</mapper>
